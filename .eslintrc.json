{
  "env": {
    "browser": true,
    "commonjs": true
  },
  "parserOptions": {
    "ecmaVersion":6,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "quotes": [
      "warn",
      "single"
    ],
    "semi": [
      "warn",
      "always"
    ],
    "no-console": "warn"
  },
  "globals": {
    "ga": true
  }
}
